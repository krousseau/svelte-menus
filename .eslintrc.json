{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": ["eslint:recommended"],
    "overrides": [
      {
        "files": ["**/*.svelte"],
        "processor": "svelte3/svelte3"
      }
    ],
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["svelte3", "@typescript-eslint"],
    "settings": {
        "svelte3/typescript": true
    }
}